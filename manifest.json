{
  "manifest_version": 2,
  "name": "GPT-3 Chat",
  "version": "1.0",
  "permissions": ["activeTab", "https://api.openai.com/"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
